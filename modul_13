price = 0
tickets = int(input('Введите количество билетов '))

for i in range(tickets):
    i += 1
    while True:
        try:
            age_for_ticket = input(f'Для какого возраста билет №{i}? ')
            age_for_ticket = int(age_for_ticket)
            if age_for_ticket < 18:
                print('Билет бесплатный')
            elif 25 > age_for_ticket >= 18:
                price += 990
                print('Стоимость билета: 990 руб.')
            else:
                price += 1390
                print('Стоимость билета: 1390 руб.')
        finally:
            print('Если вы покупаете более трех билетов, вам будет предоставлена скидка 10%')
if tickets >= 3:
    price = price - ((price / 100) * 10)
    print(f'Стоимость билетов {price} со скидкой 10%')
else:
    print(f'Стоимость билетов {price}')

